        var carousel = $(".carousel"),
    items = $(".item"),
    currdeg  = 0;

$(".next").on("click", { d: "n" }, rotate);
$(".prev").on("click", { d: "p" }, rotate);

function rotate(e){
  if(e.data.d=="n"){
    currdeg = currdeg - 60;
  }
  if(e.data.d=="p"){
    currdeg = currdeg + 60;
  }
  carousel.css({
    "-webkit-transform": "rotateY("+currdeg+"deg)",
    "-moz-transform": "rotateY("+currdeg+"deg)",
    "-o-transform": "rotateY("+currdeg+"deg)",
    "transform": "rotateY("+currdeg+"deg)"
  });
    items.css({
    "-webkit-transform": "rotateY("+(-currdeg)+"deg)",
    "-moz-transform": "rotateY("+(-currdeg)+"deg)",
    "-o-transform": "rotateY("+(-currdeg)+"deg)",
    "transform": "rotateY("+(-currdeg)+"deg)"
  });
}

document.addEventListener("DOMContentLoaded", function() {
    const updateDataButton = $("#updateDataButton");
    
    $("#updateDataButton").click(async function() {
      try {
        const result = await axios("https://api.binance.com/api/v3/ticker/price"); // Виконуємо запит до API
        
        const currentDateTime = new Date();
        currentDateTime.toLocaleString('uk-UA', { timeZone: 'Europe/Kiev' });

        const day = currentDateTime.getDate();
        const month = currentDateTime.getMonth() + 1; // Місяць починається з 0
        const year = currentDateTime.getFullYear();
        let hours = currentDateTime.getHours().toString(); // Замінено 'const' на 'let'
        let minutes = currentDateTime.getMinutes().toString();
        let seconds = currentDateTime.getSeconds().toString();
            if (hours < 10) {
                hours = '0' + hours;
            }
            if (minutes < 10) {
                 minutes = '0' + minutes;
            }
            if (seconds < 10) {
                seconds = '0' + seconds;
            }
    $(".dateTime").text("Last update:" + hours + ":" + minutes + ":" + seconds);
        // Оновлюємо відображені дані на сторінці


    var a = $(".axiosData1").text();
    var a1 = result.data[11].price.slice(0, -6) - a ; 
    $(".diffA").text(a1.toFixed(2));
    if(a1 >= 0){
        $(".diffA").removeClass("lower");
        $(".diffA").addClass("higher");
    }
    if(a1 < 0){
        $(".diffA").removeClass("higher");
        $(".diffA").addClass("lower");
    }
    
    var b = $(".axiosData2").text();
    var b1 = result.data[12].price.slice(0, -6) - b ; 
